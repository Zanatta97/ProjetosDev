<?xml version="1.0" encoding="utf-8"?>
<MCR_WEB>
<DATABASE NAME="Lancamentos" TITLE="Banco de Dados">
	<TABLE NAME="CLIENTES" TITLE="CLIENTES">
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="TRUE" SEQUENCE="1" NAME="ID_CLIENTE" TITLE="ID_CLIENTE" />
		<FIELD TYPE="TEXT" SIZE="500" NAME="NOME_CLIENTE" TITLE="NOME_CLIENTE" />
		<FIELD TYPE="TEXT" SIZE="14" MASK="999.999.999-99" NAME="CPF_CLIENTE" TITLE="CPF_CLIENTE" />
		<FIELD TYPE="TEXT" SIZE="18" MASK="99.999.999/9999-99" NAME="CNPJ_CLIENTE" TITLE="CNPJ_CLIENTE" />
		<FIELD TYPE="TEXT" SIZE="500" NAME="ENDERECO_CLIENTE" TITLE="ENDERECO_CLIENTE" />
		<FIELD TYPE="TEXT" SIZE="500" NAME="COMPLEMENTO_CLIENTE" TITLE="COMPLEMENTO_CLIENTE" />
		<FIELD TYPE="TEXT" SIZE="500" NAME="BAIRRO_CLIENTE" TITLE="BAIRRO_CLIENTE" />
		<FIELD TYPE="TEXT" SIZE="500" NAME="CIDADE_CLIENTE" TITLE="CIDADE_CLIENTE" />
		<FIELD TYPE="TEXT" SIZE="500" NAME="UF_CLIENTE" TITLE="UF_CLIENTE" />
		<FIELD TYPE="TEXT" SIZE="10" MASK="99.999-999" NAME="CEP_CLIENTE" TITLE="CEP_CLIENTE" />
		<FIELD TYPE="TEXT" SIZE="14" MASK="(99) 9999-9999" NAME="TELEFONE_CLIENTE" TITLE="TELEFONE_CLIENTE" />
		<FIELD TYPE="TEXT" SIZE="15" MASK="(99) 99999-9999" NAME="CELULAR_CLIENTE" TITLE="CELULAR_CLIENTE" />
		<FIELD TYPE="TEXT" SIZE="10" NAME="STATUS_CLIENTE" TITLE="STATUS_CLIENTE" />
		<FIELD TYPE="NUMBER" SIZE="15" MASK="999.999.999.999.999" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" NAME="QT_LANCAMENTOS_CLIENTE" TITLE="QT_LANCAMENTOS_CLIENTE" />
		<INDEX NAME="PK_CLIENTES" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90" TITLE="PK_CLIENTES">
			<FIELD NAME="ID_CLIENTE" />
		</INDEX>
	</TABLE>
	<TABLE NAME="FORNECEDORES" TITLE="FORNECEDORES">
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="TRUE" SEQUENCE="1" NAME="ID_FORNEC" TITLE="ID_FORNEC" />
		<FIELD TYPE="TEXT" SIZE="500" NAME="NOME_FORNEC" TITLE="NOME_FORNEC" />
		<FIELD TYPE="TEXT" SIZE="14" MASK="999.999.999-99" NAME="CPF_FORNEC" TITLE="CPF_FORNEC" />
		<FIELD TYPE="TEXT" SIZE="18" MASK="99.999.999/9999-99" NAME="CNPJ_FORNEC" TITLE="CNPJ_FORNEC" />
		<FIELD TYPE="TEXT" SIZE="500" NAME="ENDERECO_FORNEC" TITLE="ENDERECO_FORNEC" />
		<FIELD TYPE="TEXT" SIZE="500" NAME="COMPLEMENTO_FORNEC" TITLE="COMPLEMENTO_FORNEC" />
		<FIELD TYPE="TEXT" SIZE="500" NAME="BAIRRO_FORNEC" TITLE="BAIRRO_FORNEC" />
		<FIELD TYPE="TEXT" SIZE="500" NAME="CIDADE_FORNEC" TITLE="CIDADE_FORNEC" />
		<FIELD TYPE="TEXT" SIZE="10" NAME="UF_FORNEC" TITLE="UF_FORNEC" />
		<FIELD TYPE="TEXT" SIZE="10" MASK="99.999-999" NAME="CEP_FORNEC" TITLE="CEP_FORNEC" />
		<FIELD TYPE="TEXT" SIZE="14" MASK="(99) 9999-9999" NAME="TELEFONE_FORNEC" TITLE="TELEFONE_FORNEC" />
		<FIELD TYPE="TEXT" SIZE="15" MASK="(99) 99999-9999" NAME="CELULAR_FORNEC" TITLE="CELULAR_FORNEC" />
		<FIELD TYPE="TEXT" SIZE="10" NAME="STATUS_FORNEC" TITLE="STATUS_FORNEC" />
		<FIELD TYPE="NUMBER" SIZE="15" MASK="999.999.999.999.999" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" NAME="QT_LANCAMENTOS_FORNEC" TITLE="QT_LANCAMENTOS_FORNEC" />
		<FIELD TYPE="NUMBER" SIZE="14" MASK="999.999.999.999,99" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" NAME="SALDO_ATUAL_FORNEC" TITLE="SALDO_ATUAL_FORNEC" />
		<FIELD TYPE="NUMBER" SIZE="14" MASK="999.999.999.999,99" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" NAME="JUROS_FORNEC" TITLE="JUROS_FORNEC" />
		<FIELD TYPE="DATE" MASK="dd/MM/yyyy" NAME="ULTIMO_JUROS_FORNEC" TITLE="ULTIMO_JUROS_FORNEC" />
		<FIELD TYPE="DATE" MASK="dd/MM/yyyy" NAME="PROXIMA_ATT_FORNEC" TITLE="PROXIMA_ATT_FORNEC" />
		<FIELD TYPE="DATE" MASK="dd/MM/yyyy" NAME="ULTIMO_SAQUE_FORNEC" TITLE="ULTIMO_SAQUE_FORNEC" />
		<FIELD TYPE="TEXT" SIZE="10" NAME="ULTIMO_DEPOSITO_FORNEC" TITLE="ULTIMO_DEPOSITO_FORNEC" />
		<INDEX NAME="PK_FORNECEDORES" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90" TITLE="PK_FORNECEDORES">
			<FIELD NAME="ID_FORNEC" />
		</INDEX>
	</TABLE>
	<TABLE NAME="LANC_CLIENTES" TITLE="LANC_CLIENTES">
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="TRUE" SEQUENCE="1" NAME="ID_LANC_CLI" TITLE="ID_LANC_CLI" />
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="FALSE" NAME="ID_CLIENTE" TITLE="ID_CLIENTE" />
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" NAME="SEQUENCIAL_LANC_CLI" TITLE="SEQUENCIAL_LANC_CLI" />
		<FIELD TYPE="NUMBER" SIZE="14" MASK="999.999.999.999,99" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" NAME="VALOR_LANC_CLI" TITLE="VALOR_LANC_CLI" />
		<FIELD TYPE="NUMBER" SIZE="14" MASK="999.999.999.999,99" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" NAME="TAXA_LANC_CLI" TITLE="TAXA_LANC_CLI" />
		<FIELD TYPE="DATE" MASK="dd/MM/yyyy" NAME="DATA_LANC_CLI" TITLE="DATA_LANC_CLI" />
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" NAME="PRAZO_LANC_CLI" TITLE="PRAZO_LANC_CLI" />
		<FIELD TYPE="DATE" MASK="dd/MM/yyyy" NAME="DATA_FINAL_LANC_CLI" TITLE="DATA_FINAL_LANC_CLI" />
		<FIELD TYPE="NUMBER" SIZE="14" MASK="999.999.999.999,99" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" NAME="VOLTA_LANC_CLI" TITLE="VOLTA_LANC_CLI" />
		<INDEX NAME="PK_LANC_CLIENTES" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90" TITLE="PK_LANC_CLIENTES">
			<FIELD NAME="ID_LANC_CLI" />
		</INDEX>
		<RELATION FOREIGNTABLE="CLIENTES" BASETABLE="LANC_CLIENTES" TYPE="CONSTRAINT" CARDINALITY="1-N" NAME="FK_LANC_CLIENTES">
			<RELATIONOF FOREIGNFIELD="ID_CLIENTE" BASEFIELD="ID_CLIENTE" NAME="ID_CLIENTE" />
		</RELATION>
	</TABLE>
	<TABLE NAME="LANC_FORNECEDOR" TITLE="LANC_FORNECEDOR">
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="TRUE" SEQUENCE="1" NAME="ID_LANC_FORNEC" TITLE="ID_LANC_FORNEC" />
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="FALSE" NAME="ID_FORNEC" TITLE="ID_FORNEC" />
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" NAME="SEQUENCIAL_LANC_FORNEC" TITLE="SEQUENCIAL_LANC_FORNEC" />
		<FIELD TYPE="NUMBER" SIZE="14" MASK="999.999.999.999,99" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" NAME="VALOR_LANC_FORNEC" TITLE="VALOR_LANC_FORNEC" />
		<FIELD TYPE="NUMBER" SIZE="14" MASK="999.999.999.999,99" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" NAME="VALOR_JUROS_LANC_FORNEC" TITLE="VALOR_JUROS_LANC_FORNEC" />
		<FIELD TYPE="NUMBER" SIZE="14" MASK="999.999.999.999,99" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" NAME="JUROS_ACUMULADOS_LANC_FORNEC" TITLE="JUROS_ACUMULADOS_LANC_FORNEC" />
		<FIELD TYPE="DATE" MASK="dd/MM/yyyy" NAME="DATA_PRIMEIRO_LANC_FORNEC" TITLE="DATA_PRIMEIRO_LANC_FORNEC" />
		<FIELD TYPE="DATE" MASK="dd/MM/yyyy" NAME="DATA_PROXIMO_LANC_FORNEC" TITLE="DATA_PROXIMO_LANC_FORNEC" />
		<FIELD TYPE="DATE" MASK="dd/MM/yyyy" NAME="DATA_ULTIMO_LANC_FORNEC" TITLE="DATA_ULTIMO_LANC_FORNEC" />
		<INDEX NAME="PK_LANC_FORNECEDOR" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90" TITLE="PK_LANC_FORNECEDOR">
			<FIELD NAME="ID_LANC_FORNEC" />
		</INDEX>
		<RELATION FOREIGNTABLE="FORNECEDORES" BASETABLE="LANC_FORNECEDOR" TYPE="CONSTRAINT" CARDINALITY="1-N" NAME="FK_LANC_FORNECEDOR">
			<RELATIONOF FOREIGNFIELD="ID_FORNEC" BASEFIELD="ID_FORNEC" NAME="ID_FORNEC" />
		</RELATION>
	</TABLE>
	<TABLE NAME="MOVIMENTACAO_FORNECEDOR" TITLE="MOVIMENTACAO_FORNECEDOR">
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="TRUE" SEQUENCE="1" NAME="ID_MOV" TITLE="ID_MOV" />
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="FALSE" NAME="ID_FORNEC" TITLE="ID_FORNEC" />
		<FIELD TYPE="TEXT" SIZE="10" NAME="TIPO_MOV" TITLE="TIPO_MOV" />
		<FIELD TYPE="NUMBER" SIZE="14" MASK="999.999.999.999,99" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" NAME="VALOR_MOV" TITLE="VALOR_MOV" />
		<FIELD TYPE="NUMBER" SIZE="14" MASK="999.999.999.999,99" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" NAME="SALDO_ANTERIOR_MOV" TITLE="SALDO_ANTERIOR_MOV" />
		<FIELD TYPE="NUMBER" SIZE="14" MASK="999.999.999.999,99" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" NAME="SALDO_NOVO_MOV" TITLE="SALDO_NOVO_MOV" />
		<FIELD TYPE="DATE" MASK="dd/MM/yyyy" NAME="DATA_MOV" TITLE="DATA_MOV" />
		<INDEX NAME="PK_MOVIMENTACAO_FORNECEDOR" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90" TITLE="PK_MOVIMENTACAO_FORNECEDOR">
			<FIELD NAME="ID_MOV" />
		</INDEX>
		<RELATION FOREIGNTABLE="FORNECEDORES" BASETABLE="MOVIMENTACAO_FORNECEDOR" TYPE="CONSTRAINT" CARDINALITY="1-N" NAME="FK_MOVIMENTACAO_FORNECEDOR">
			<RELATIONOF FOREIGNFIELD="ID_FORNEC" BASEFIELD="ID_FORNEC" NAME="ID_FORNEC" />
		</RELATION>
	</TABLE>
	<TABLE NAME="TB_LOGIN_GROUP" TITLE="Grupo de login">
		<FIELD TYPE="TEXT" SIZE="60" MASK="@!" NAME="LOGIN_GROUP_NAME" TITLE="Nome do grupo" />
		<FIELD TYPE="BOOLEAN" NAME="LOGIN_GROUP_IS_ADMIN" TITLE="Grupo tipo administração" />
		<INDEX NAME="LOGIN_GROUP_PK" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90" TITLE="Por nome">
			<FIELD NAME="LOGIN_GROUP_NAME" />
		</INDEX>
		<RELATION FOREIGNTABLE="TB_LOGIN_RULE" BASETABLE="TB_LOGIN_GROUP" TYPE="CONSTRAINT" CARDINALITY="1-N" NAME="TB_LOGIN_GROUP_RULE">
			<RELATIONOF FOREIGNFIELD="LOGIN_GROUP_NAME" BASEFIELD="LOGIN_GROUP_NAME" NAME="LOGIN_GROUP_NAME" />
		</RELATION>
		<RELATION FOREIGNTABLE="TB_LOGIN_USER" BASETABLE="TB_LOGIN_GROUP" TYPE="CONSTRAINT" CARDINALITY="1-N" NAME="TB_LOGIN_GROUP_USER">
			<RELATIONOF FOREIGNFIELD="LOGIN_GROUP_NAME" BASEFIELD="LOGIN_GROUP_NAME" NAME="LOGIN_GROUP_NAME" />
		</RELATION>
	</TABLE>
	<TABLE NAME="TB_LOGIN_RULE" TITLE="Regras para login">
		<FIELD TYPE="TEXT" SIZE="8" MASK="@!" NAME="LOGIN_RULE_PROJECT" TITLE="Id do projeto" />
		<FIELD TYPE="TEXT" SIZE="60" MASK="@!" NAME="LOGIN_GROUP_NAME" TITLE="Nome do grupo" />
		<FIELD TYPE="TEXT" SIZE="100" NAME="LOGIN_RULE_OBJECT" TITLE="Objeto da regra" />
		<FIELD TYPE="TEXT" SIZE="100" NAME="LOGIN_RULE_PERMISSIONS" TITLE="Permissões da regra" />
		<INDEX NAME="LOGIN_RULE_PK" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90" TITLE="Por projeto/grupo/objeto">
			<FIELD NAME="LOGIN_RULE_PROJECT" />
			<FIELD NAME="LOGIN_GROUP_NAME" />
			<FIELD NAME="LOGIN_RULE_OBJECT" />
		</INDEX>
	</TABLE>
	<TABLE NAME="TB_LOGIN_USER" TITLE="Usuário de login">
		<FIELD TYPE="TEXT" SIZE="60" MASK="@!" NAME="LOGIN_GROUP_NAME" TITLE="Nome do grupo" />
		<FIELD TYPE="TEXT" SIZE="40" MASK="@!" NAME="LOGIN_USER_LOGIN" TITLE="Login" />
		<FIELD TYPE="TEXT" SIZE="40" MASK="@!" NAME="LOGIN_USER_PASSWORD" TITLE="Senha" />
		<FIELD TYPE="TEXT" SIZE="60" MASK="@!" NAME="LOGIN_USER_NAME" TITLE="Nome do usuário" />
		<FIELD TYPE="MEMO" MASK="@!" NAME="LOGIN_USER_OBS" TITLE="Observações" />
		<INDEX NAME="LOGIN_USER_PK" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90" TITLE="Por login">
			<FIELD NAME="LOGIN_USER_LOGIN" />
		</INDEX>
	</TABLE>
</DATABASE>
</MCR_WEB>
